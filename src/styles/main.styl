.menu-link
  display block
  font-family fontBold
  font-size 24px
  letter-spacing 0.1em
  color colorBlack
  text-transform uppercase
  padding 20px 0 16px 50px
  margin 10px 0
  &:hover
  &:focus
    background url("../assets/images/gradient-menu.png")
    background-size cover

.footer-menu-link
  font-family fontRegular
  font-size 16px
  line-height 1.7
  color colorBlack

.link-social
  display inline-flex
  margin 10px 0
  opacity 0.75
  +.link-social
    margin-left 30px
  &:hover
  &:focus
    opacity 1

.link-map
  display flex
  align-items center
  font-family fontRegular
  font-size 16px
  line-height 1.7
  color colorDarkGrey
  text-decoration underline
  &:hover
  &:focus
    text-decoration underline
  &:before
    content url("../assets/images/icons/showonmap.svg")
    width 16px
    height 16px
    margin-right 7px

.link-play
  font-family fontExtraBold
  font-size 14px
  color colorBlack
  letter-spacing 0.3em
  text-transform uppercase
  &:after
    content ""
    display inline-block
    width 0
    height 0
    border-top 5px solid transparent
    border-bottom 5px solid transparent
    border-left 10px solid colorBlack
    margin-left 10px

.form-link
  display inline
  text-decoration underline
  &:hover
  &:focus
    text-decoration underline

.link-underline
  text-decoration underline
  &:hover
  &:focus
    text-decoration underline

.link-bordered
  display flex
  align-items center
  justify-content center
  width 300px
  height 60px
  border 1px solid colorGrey
  font-family fontBold
  font-size 14px
  letter-spacing 2px
  color colorBlack
  text-transform uppercase
  margin 50px auto 0

/** Image container **/
.img-container
  position relative

.img-gradient
  position absolute
  width 100%
  height 100%
  gradient-bg()
  opacity 0.3
  transition all 0.5s ease-in-out
  &.top-left
    top -20px
    left -20px
    &:hover
      top 0
      left 0
  &.bottom-right
    bottom -20px
    right -20px
    &:hover
      bottom 0
      right 0
  &.top-right
    top -20px
    right -20px
    &:hover
      top 0
      right 0

