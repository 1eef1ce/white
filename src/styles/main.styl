fontRegular = CodecWarmRegular
fontBold = CodecWarmBold
fontExtraBold = CodecWarmExtraBold
fontMedium = OswaldMedium
fontLight = OswaldLight

colorBlack = #000000
colorWhite = #ffffff
colorGrey = #424242
colorLightenGrey = #b8b8b8
colorLightGrey = #616161
colorDarkGrey = #333333

gradient-bg()
  background linear-gradient(116.56deg, #9CEEE5 33.33%, #FEB6C5 53.12%, #FDD3B9 72.57%, #F5FABD 100%)

gradient-color()
  background linear-gradient(168.14deg, #9CEEE5 1.67%, #FEB6C5 30.86%, #FDD3B9 59.54%, #F5FABD 100%)

gradient-strike()
  background linear-gradient(174.15deg, rgba(156, 238, 229, 0.5) 15.61%,
                                        rgba(254, 182, 197, 0.5) 35.19%,
                                        rgba(253, 211, 185, 0.5) 54.42%,
                                        rgba(245, 250, 189, 0.5) 81.55%)

@font-face
  font-family OswaldMedium
  src url(../assets/fonts/Oswald-Medium.woff2) format('woff2'),
  url(../assets/fonts/Oswald-Medium.woff) format('woff')

@font-face
  font-family OswaldLight
  src url(../assets/fonts/Oswald-Light.woff2) format('woff2'),
  url(../assets/fonts/Oswald-Light.woff) format('woff')

@font-face
  font-family CodecWarmRegular
  src url(../assets/fonts/CodecWarm-Regular.woff2) format('woff2'),
  url(../assets/fonts/CodecWarm-Regular.woff) format('woff')

@font-face
  font-family CodecWarmBold
  src url(../assets/fonts/CodecWarm-Bold.woff2) format('woff2'),
  url(../assets/fonts/CodecWarm-Bold.woff) format('woff')

@font-face
  font-family CodecWarmExtraBold
  src url(../assets/fonts/CodecWarm-ExtraBold.woff2) format('woff2'),
  url(../assets/fonts/CodecWarm-ExtraBold.woff) format('woff')

/** General **/
.row
  &:before
  &:after
    display inline-block !important
    content normal

input
textarea
  resize none
  &:focus
    box-shadow none!important

button
a
  display inline-block
  color inherit
  text-decoration none
  &:hover
  &:focus
    outline none
    box-shadow none
    color inherit
    text-decoration none

button
  padding 0
  border 0

img
  display block
  margin auto
  max-width 100%
  max-height 100%

ul
  list-style none
  margin 0
  padding 0

s
  text-decoration none
  gradient-strike()

body
  position relative
  font-family fontRegular, Arial, sans-serif
  font-size 16px
  color colorBlack
  line-height 1
  background-color colorWhite

/** Containers **/
.container
  max-width 80%
  padding 0

  @media (min-width 640px)
    max-width 600px

  @media (min-width 1000px)
    max-width 960px

  @media (min-width 1600px)
    max-width 1200px
    margin auto

.container-inner
  max-width 80%
  margin auto

  @media (min-width 640px)
    max-width 600px

.row
  margin-left 0
  margin-right 0

  @media (min-width 640px)
    margin-left -20px
    margin-right -20px

  @media (min-width 1600px)
    margin-left -50px
    margin-right -50px

.col
[class*="col-"]
  padding-left 0
  padding-right 0

  @media (min-width 640px)
    padding-left 20px
    padding-right 20px

  @media (min-width 1600px)
    padding-left 50px
    padding-right 50px

section
  margin-bottom 100px

  @media (min-width 640px)
    margin-bottom 120px

  @media (min-width 1000px)
    margin-bottom 150px

  @media (min-width 1600px)
    margin-bottom 200px

/** Headings **/
h1,h2,h3,h4,
.h1,.h2,.h3,.h4
  color colorBlack
  letter-spacing 0.1em
  text-transform uppercase
  font-weight normal
  margin 0
  padding 0!important
  & span
    font-family fontLight

h1
.h1
  position relative
  font-family fontMedium
  font-size 33px
  line-height 1.4
  margin-bottom 0
  &:before
    content ""
    position absolute
    width w = 28px
    height 2px
    top 0.575em
    left -(w + 10px)
    background colorBlack
  .section-main &
    margin-bottom 79px
    &:before
      content none

  @media (min-width 640px)
    .section-main &
      margin-left 38px
      &:before
        content ""
        position absolute
        width w = 28px
        height 2px
        top 0.65em
        left -(w + 10px)
        background colorBlack

  @media (min-width 1000px)
    font-size 48px
    &:before
      top 0.675em
      width w = 50px
      left -(w + 10px)
    .section-main &
      font-size 72px
      margin-left 100px
      &:before
        width w = 70px
        left -(w + 30px)

  @media (min-width 1600px)
    &:before
      width w = 50px
      left -(w + 20px)

.section-title
  display flex
  align-items center
  background url("../assets/images/gradient-title.png")
  background-size cover
  padding 54px 0

  @media (min-width 640px)
    position relative
    max-width 600px
    margin auto
    .container
      margin 0 48px

  @media (min-width 1000px)
    max-width 960px
    padding 68px 0
    font-size 72px
    .container
      margin 0 80px

  @media (min-width 1600px)
    max-width 1400px
    .container
      margin auto

h2
.h2
  position relative
  font-family fontMedium
  font-size 24px
  line-height 1.3
  margin-bottom 30px
  &:before
    content ""
    position absolute
    width w = 70px
    height 1px
    top 0.575em
    left -(w + 5px)
    background colorLightenGrey

  @media (min-width 640px)
    &:before
      width w = 32px
      left -(w + 5px)

  @media (min-width 1000px)
    font-size 36px
    margin-bottom 40px
    &:before
      width w = 70px
      left -(w + 10px)

  @media (min-width 1600px)
    &:before
      width w = 70px
      left -(w + 30px)

h3
.h3
  font-family fontBold
  font-size 18px
  line-height 1.3
  margin-bottom 20px

  @media (min-width 1000px)
    font-size 24px

h4
.h4
  font-family fontBold
  font-size 17px
  line-height 1.5
  margin-bottom 20px

  @media (min-width 1000px)
    font-size 20px
    line-height 1.3

.h4-var
  font-family fontBold
  font-size 17px
  line-height 1.3
  letter-spacing 0.1em
  text-transform uppercase
  color colorLightGrey
  margin-bottom 14px

/** Regular text **/
p
.text-regular
  font-family fontRegular
  font-size 16px
  line-height 1.7
  color colorBlack
  margin-bottom 24px

/** Buttons **/
.btn
  display flex
  align-items center
  justify-content center
  font-family fontRegular
  font-size 14px
  letter-spacing 2px
  text-transform uppercase
  color colorWhite
  width 258px
  height 56px
  background colorGrey
  border 0
  border-radius 0
  padding 0
  &:hover
  &:focus
    color colorBlack
    text-decoration none
    box-shadow none
    background url("../assets/images/gradient-btn.png")

.btn-link
  background colorGrey url("../assets/images/icons/arrow-white-16px.svg") no-repeat right 30px center
  background-size 16px
  justify-content start
  padding 0 30px
  &:hover
  &:focus
    color colorGrey
    background url("../assets/images/gradient-btn.png"), url("../assets/images/icons/arrow-dark-26px.svg") no-repeat right 30px center

.btn-small
  width 184px
  height 40px
  font-size 12px
  letter-spacing 0.1em
  background-position right 15px center
  padding 0 15px
  &:hover
  &:focus
    background url("../assets/images/gradient-btn.png"), url("../assets/images/icons/arrow-dark-18px.svg") no-repeat right 15px center

  @media (min-width 1000px)
    width 258px
    height 56px
    font-size 14px
    letter-spacing 2px
    background-position right 30px center
    padding 0 30px

.btn-header
  display none
  margin-right 55px

  @media (min-width 1000px)
    display flex

.btn-in
  position absolute
  bottom 20px
  left 0
  right 0
  margin auto
  .section-men-women &
    bottom 40px

  @media (min-width 1000px)
    bottom 50px

.btn-tab
  display flex
  align-items center
  justify-content center
  width 100%
  height 55px
  background colorWhite
  border 1px solid colorGrey
  font-family fontRegular
  font-size 14px
  color colorGrey
  letter-spacing 2px
  text-transform uppercase
  &:hover
  &:focus
  &.active
    background colorGrey
    color colorWhite

  @media (min-width 640px)
    width 50%

  @media (min-width 1000px)
    width 258px

.btn-dropdown
  position relative
  display flex
  align-items center
  width 100%
  max-width 348px
  height 60px
  padding 0 20px
  background colorWhite
  border 1px solid colorLightenGrey
  font-family fontBold
  font-size 15px
  letter-spacing 0.1em
  text-transform uppercase
  color rgba(0, 0, 0, 0.3)
  margin-bottom 40px
  &:after
    position absolute
    right 14px
    content ""
    width 0
    height 0
    border-left 6px solid transparent
    border-right 6px solid transparent
    border-top 10px solid rgba(0, 0, 0, 0.3)
  &:hover
  &:focus
    color rgba(0, 0, 0, 0.3)
  &[aria-expanded="true"]
    &:after
      border-top 0
      border-bottom  10px solid rgba(0, 0, 0, 0.3)

  @media (min-width 1000px)
    display none

.btn-square
  width 40px
  height 40px
  +.btn-square
    margin-left 10px

  @media (min-width 640px)
    +.btn-square
      margin-left 20px

.btn-marker
  background-image url("../assets/images/icons/marker.svg")
  background-repeat no-repeat
  background-position center

.btn-phone
  background-image url("../assets/images/icons/phone.svg")
  background-repeat no-repeat
  background-position center

.btn-signup
  width 200px
  height 40px
  font-size 13px

/** Links **/
.link-lang
  font-family fontBold
  font-size 14px
  color colorLightenGrey
  text-transform uppercase
  margin 0 0.35em
  &:hover
  &.active
    color colorBlack
    border-bottom 2px solid colorBlack

.link-phone
